{
	"$schema": "https://opencode.ai/config.json",
	"model": "copilot/claude-opus-4-5",
	"plugin": [
		"oh-my-opencode"
	],
	"provider": {
		"openai": {
			"models": {
				"gpt-5": {
					"options": {
						"reasoningEffort": "high",
						"textVerbosity": "low",
						"reasoningSummary": "auto",
						"include": [
							"reasoning.encrypted_content"
						],
					},
				},
			},
		},
	},
	"instructions": [
		"beads.md"
	],
	"keybinds": {
		"leader": "ctrl+f"
	}
}
